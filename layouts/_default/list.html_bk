{{ define "title" }}{{ if .IsHome }}Home{{ else }}{{ .Title }}{{ end }}{{ end }}
{{ define "main" }}
  <h1>{{ if .IsHome }}Latest Posts{{ else }}{{ .Title }}{{ end }}</h1>
  <ul class="post-list">
    {{ range .Paginator.Pages }}
      <li>
        <a href="{{ .RelPermalink }}">{{ .Title }}</a>
        <span class="date">{{ .Date.Format "2006-01-02" }}</span>
      </li>
    {{ else }}
      <li>No posts yet.</li>
    {{ end }}
  </ul>
{{ end }}